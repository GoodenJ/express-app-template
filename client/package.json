{
  "name": "client",
  "version": "1.0.0",
  "description": "Express App Template Client Source Code",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development && node index.js",
    "postinstall": "npm run build",
    "dev": "webpack-dev-server --mode none --hot --host 0.0.0.0",
    "build": "set NODE_ENV=production && webpack --mode production",
    "build:dev": "set NODE_ENV=development && webpack --mode development",
    "build:stable": "set NODE_ENV=stable && webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/tests/setup.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/mock/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/src/tests/"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "bootstrap": "^3.3.7",
    "cross-fetch": "^2.2.2",
    "express": "^4.16.4",
    "format-currency": "^1.0.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-dropzone": "^8.0.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
